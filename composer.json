{
    "name": "jackwestin/laravel-discord-notifier",
    "description": "Send Discord messages through webhook with Discord",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Mehran Rasuli",
            "email": "mehrunrasoli@gmail.com"
        }
    ],
    "homepage": "https://github.com/mehranrasuli/laravel-discord-notifier",
    "keywords": ["Laravel", "notifier", "discord"],
    "require": {
        "illuminate/support": "~6|~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Jackwestin\\Notifier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jackwestin\\Notifier\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jackwestin\\Notifier\\NotifierServiceProvider"
            ],
            "aliases": {
                "Discord": "Discord"
            }
        }
    }
}
